{
  "name"        : "pkgcloud",
  "description" : 
    "An infrastructure-as-a-service agnostic cloud library for node.js",
  "version"     : "0.3.0",
  "author"      : "Nodejitsu Inc <info@nodejitsu.com",
  "contributors": [
    { "name": "Charlie Robbins", "email": "charlie@nodejitsu.com" },
    { "name": "Nuno Job",        "email": "nunojobpinto@gmail.com" }
